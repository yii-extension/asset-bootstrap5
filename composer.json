{
    "name": "yii-extension/asset-bootstrap5",
    "type": "library",
    "description": "Bootstrap5 Asset for Yii3",
    "keywords": [
        "yii3",
        "bootstrap5",
        "asset"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "foxy/foxy": "dev-patch-1 as dev-master",
        "yiisoft/assets": "^1.0.0",
        "yiisoft/files": "^1.0.0"
    },
    "require-dev": {
        "psr/log": "^1.1",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.4",
        "yiisoft/aliases": "^2.0.0",
        "yiisoft/di": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Asset\\Bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Asset\\Bootstrap5\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/SamMousa/foxy"
        }
    ]
}
