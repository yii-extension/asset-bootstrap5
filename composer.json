{
    "name": "yii-extension/asset-bootstrap5",
    "type": "library",
    "description": "Bootstrap5 Asset For Yii3",
    "keywords": [
        "yii3",
        "asset",
        "bootstrap5"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "foxy/foxy": "^1.1@dev",
        "yiisoft/assets": "^1.0.0",
        "yiisoft/files": "^1.0.0"
    },
    "require-dev": {
        "psr/log": "^1.1",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.4",
        "yiisoft/aliases": "^2.0.0",
        "yiisoft/test-support": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Asset\\Bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Asset\\Bootstrap5\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
